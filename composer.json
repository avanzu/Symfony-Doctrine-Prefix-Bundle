{
    "name": "avanzu/sf-doctrine-prefix-bundle",
    "type": "symfony-bundle",
    "keywords": ["symfony", "bundle", "doctrine", "table prefix"],
    "description": "Applies table prefixes to doctrine tables",
    "license": "MIT",
    "authors": [
        {
            "name": "Marc Bach",
            "email": "mail@avanzu.de"
        }
    ],
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "symfony/framework-bundle": ">=2.1,<3.0"
    },
    "autoload": {
        "psr-0": { "Avanzu\\Doctrine\\PrefixBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "Avanzu/Doctrine/PrefixBundle"
}
